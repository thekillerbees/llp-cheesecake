<meta name="description" content="cheesecake Store and
Dealer locator" />

		<link type="text/css" href="css/proto.css" rel="stylesheet" media="all"/>
		<link type="text/css" href="/w2gi/css/litbox.css" rel="stylesheet" media="all"/>		
		<link type="text/css" href="/w2gi/css/bubble.css" rel="stylesheet" media="all"/>		
		<link type="text/css" href="/w2gi/css/style.css" rel="stylesheet" media="all"/>		

		<script type="text/javascript" src="/w2gi/javascript/ace/1.74/prototype.js"></script>
