		<script type="text/javascript" src="/w2gi/javascript/ace/1.74/W2GI_slippymap.js"></script>

		<script type="text/javascript">
			function loadSlippyMap() {
				var options = {
					dynamicSearch: true,
					geoip: false,
					theme: null,
                                        routeStyle: {   strokeWidth: 8,
							strokeOpacity: .5,
                                                        strokeColor: '#0000FF' },
					controls: [	new OpenLayers.Control.PanZoomBar(), 
							new OpenLayers.Control.Navigation() ]
				};
				OpenLayers.IMAGE_RELOAD_ATTEMPTS = 2; 
				OpenLayers.ImgPath='/w2gi/images/zoombar/';
				SlippyMap.load('map', 'panel', options);		
			}
			
	            function clear_search_directions_box(i) {
	                if (i.value == 'Enter street address, city, state or ZIP') {
        	            i.value = '';
                	}
	                if (i.value == 'Start address') {
        	            i.value = '';
	                }
        	        if (i.value == 'End address') {
                	    i.value = '';
	                }
        	    }
			
		</script>	

	</head>
	
	<body onload="loadSlippyMap();" onunload="SlippyMap.unload();">		
		<!-- XML data island start  -->
		<xml>
			<root>
                <ajaxurl>/ajax?</ajaxurl>
                <appkey>320C479E-6D70-11DE-9D8B-E57E37ABAA09</appkey>
				<inputforms>				
					<inputform>
						<formdata id="reversegeocoder">
							<geolocs>
								<geoloc>
									<latitude></latitude>
									<longitude></longitude>
									<country></country>
								</geoloc>
							</geolocs>
						</formdata>			
					</inputform>
				</inputforms>									
                                <sources>
                                  <source>
                                    <name>SlippyMap W2GI</name>
                                    <imageurl>http://st1.slippymap.com/tiles, http://st2.slippymap.com/tiles</imageurl>
                                    <options>{map: 'smap-01'}</options>
                                  </source>
                                  <source>
                                    <name>SlippyMap Navteq</name>
                                    <imageurl>http://st1.slippymap.com/tiles, http://st2.slippymap.com/tiles</imageurl>
                                    <options>{map: 'smap-02'}</options>
                                  </source>
                                </sources>
			</root>		
		</xml>
