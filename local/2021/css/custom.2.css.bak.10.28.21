/*filters*/
#filterToggle {
    background-color: #0E283C;
    border: none;
    border-radius: 50%;
    color: #fff;
    cursor: pointer;
    height: 60px;
    position: absolute;
    top: 42px;
    transition: ease-in-out .45s all;
    width: 60px;
}

@media (min-width: 802px){
    #filterToggle {
        right: -70px;
        /*left: 10px;*/
    }
}

#filterToggle:before {
    color: #fff;
    content: '\f0ca';
    font-family: 'FontAwesome';
    font-size: 16px;
    position: relative;
    top: 2px;
    z-index: 2;
}

#filterToggle:after {
    border: 2px solid #fff;
    border-radius: 50%;
    -webkit-box-shadow: 9px 3px 10px -3px rgba(108,108,108,0.65); 
    box-shadow: 9px 3px 10px -3px rgba(108,108,108,0.65);
    content: '';
    display: block;
    height: 50px;
    left: 50%;
    position: absolute;
    top: 50%;
    transform: translate(-50%,-50%);
    width: 50px;
    z-index: 1;
}

#filterToggle span {
    color: #fff;
    display: block;
}

#filterToggle.open span {
    color: #0e283c;
}


#filterToggle.open {
    background-color: #fff;
}

#filterToggle.open:before {
    color: #0E283C;
}

#filterToggle.open:after {
    border: 2px solid #0E283C;
}

.filter_checkbox {
    display:none;
}

#filters {
    background-color: #fff;
    border-radius: 0 20px;
    -webkit-box-shadow: 1px 3px 12px -3px rgb(108,108,108,.45); 
    /*box-shadow: 9px 3px 10px -3px rgba(108,108,108,0.65);*/
    box-shadow: 1px 3px 12px -3px rgb(108,108,108,.45);
    height: 0;
    /*left: 38px;*/
    left: 463px;
    opacity: 0;
    /*overflow: auto;*/
    padding: 20px;
    position: absolute;
    top: 70px;
    transition: ease-in-out .35s all;
    width: 160px;
}

#filters:after {
    border: 2px solid #0E283C;
    border-radius: 0 20px;
    content: '';
    display: block;
    height: 0;
    left: 50%;
    pointer-events: none;
    position: absolute;
    top: 9px;
    transform: translateX(-50%);
    width: 136px;
}

#filters.open {
    height: 240px;
    
    opacity: 1;
    overflow: hidden;
    z-index: 2;
    /*top: 70px;*/
}


#filters.open:after {
    height: 220px;
}

#filters label {
    letter-spacing: .025em;
    transition: all .2s ease-in-out;
    vertical-align: bottom;
}

#filters input {
    display: none;
}

.cb_wrapper {
    margin: 0 0 16px;
}

#filters > div:first-of-type {
    margin: 0 0 10px;
}

#filters label:before {
    border: 2px solid #0E283C;
    border-radius: 4px;
    content: "";
    cursor: pointer;
    display: inline-block;
    height: 25px;
    line-height: 25px;
    margin-right: 12px;
    position: relative;
    text-align: center;
    top: 2px;
    transition: all .2s ease-in-out;
    vertical-align: inherit;
    width: 25px;
}


#filters input[type="checkbox"]:checked + label {
    color: #0E283C;
    font-weight: bold;
}

#filters input[type="checkbox"]:checked + label:before {
    background-color: #0E283C;
    color: #fff;
    content: "\f00c";
    font-family: FontAwesome;
    font-size: 14px;
    font-style: normal;
    font-weight: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

#closeFilters {
    background: transparent;
    border: none;
    border-radius: 0;
    color:  #0E283C;
    cursor: pointer;
    font-size: 26px;
    right:  16px;
    padding: 1px 6px;
    position: absolute;
    top: 12px;
    -webkit-appearance: none;
}

