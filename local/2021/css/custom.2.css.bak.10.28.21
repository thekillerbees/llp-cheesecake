}

.tab_wrapper li button:before {
    display: inline-block;
    margin: 0 16px 0 0;
}

.tab_wrapper li:first-of-type button {
    border-radius: 8px 0 0 8px;
}

.tab_wrapper li:first-of-type button:before {
    content: url(/cheesecake/2021/images/reservations.svg);
    height: 34px;
    width: 26px;
}


