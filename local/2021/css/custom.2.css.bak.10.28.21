}

.tab_wrapper li:last-of-type button:before {
    content: url(/cheesecake/2021/images/doordash.svg);
    height: 21px;
    position: relative;
    top: -4px;
    width: 35px;
}


.tab_wrapper li:last-of-type button.selected:before {
    content: url(/cheesecake/2021/images/doordash.selected.svg);
    height: 21px;
    width: 35px;
}

.tab_wrapper li:last-of-type button {
    border-radius: 0 8px 8px 0;
}

.tab_wrapper li button span {
    display: inline-block;
    line-height: normal;
    vertical-align: bottom;
}

.tab_wrapper li:nth-of-type(2) button span {
    position: relative;
    top: -5px;
}

