.sr-only {
    position: absolute;
    width: 1px;
    height: 1px;
    padding: 0;
    margin: -1px !important;
    overflow: hidden;
    clip: rect(0,0,0,0);
    border: 0;
}

.sr-only-focusable:active,.sr-only-focusable:focus{
    position:absolute;
    top:10px; left:10px;
    width:auto;
    height:auto;
    margin:0;
    overflow:visible;
    clip:auto;
    z-index: 1;
}

