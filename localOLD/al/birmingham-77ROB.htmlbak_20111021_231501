<script type="text/javascript">
			function loadSlippyMap() {
				var options = {
					dynamicSearch: true,
					geoip: false,
					theme: null,
                                        routeStyle: {   strokeWidth: 8,
							strokeOpacity: .5,
                                                        strokeColor: '#0000FF' },
					controls: [	new OpenLayers.Control.PanZoomBar(), 
							new OpenLayers.Control.Navigation() ],
					maxExtent: new OpenLayers.Bounds(-170,10,-50,75)
				};
				OpenLayers.IMAGE_RELOAD_ATTEMPTS = 2; 
				OpenLayers.ImgPath='http://hosted.where2getit.com/cheesecake/images/zoombar/';
				SlippyMap.load('map', 'panel', options);		
			}
			
	            function clear_search_directions_box(i) {
	                if (i.value == 'Enter street address, city, state or ZIP') {
        	            i.value = '';
                	}
	                if (i.value == 'Start address') {
        	            i.value = '';
	                }
        	        if (i.value == 'End address') {
                	    i.value = '';
	                }
        	    }
				
				function printprepare() {
					clear_search_directions_box($('inputaddress'));
					clear_search_directions_box($('startaddress'));
					clear_search_directions_box($('endaddress'));
			}
			

			 function viewDirectionsToForm( poiNum )
		    {
    		    $( 'directions_links_' + poiNum ).hide();
		        $( 'directions_form_' + poiNum ).show();
		    
		        $( 'direction_display_' + poiNum ).update( 'From:' );
		        $( 'direction_link_' + poiNum ).update( 'To' );
		    }
		    
		    function viewDirectionsFromForm( poiNum )
		    {
    		    $( 'directions_links_' + poiNum ).hide();
		        $( 'directions_form_' + poiNum ).show();
		    
		        $( 'direction_display_' + poiNum ).update( 'To:' );
		        $( 'direction_link_' + poiNum ).update( 'From' );
		    }

			var litboxTopHeight = '18px';
		</script>	
