    <script>

    function generate_directions(startaddress, endaddress){
    alert(startaddress);
    endaddress = endaddress.replace("#","poundsign");

    if(bubbleValidate(startaddress)){
      //$('directions_url').setAttribute('url', 'http://hosted.where2getit.com/cheesecake/directions_popup.html?form=driving_directions&addressline=' + startaddress + '&addressline=' + '{{{latitude}}},{{{longitude}}}' + '&addresslineDisplay=' + endaddress);

        //performClick($('directions_url'));

      var url = 'http://hosted.where2getit.com/cheesecake/directions_popup.html?form=driving_directions&addressline=' + startaddress + '&addressline=' + '{{{latitude}}},{{{longitude}}}' + '&addresslineDisplay=' + endaddress;
      window.open(url,'website','toolbar=1,location=1,directories=1,status=1,menubar=1,scrollbars=1,resizable=1');
    }
  }


  function bubbleValidate(bubbleInput) {
    if(bubbleInput.length > 0 && bubbleInput.length < 5 && !isNaN(bubbleInput) ) {
      //ace.util.alert("Please enter a valid ZIP code.");
      alert ("Please enter a valid ZIP.");
      return false;
    }

    if(bubbleInput.length == 0) {
      alert("Please enter a starting address.");
      return false;
    }

    return true;
  }

  function performClick(node) {
    if((navigator.userAgent.match(/Safari/i))) {
        var evObj = document.createEvent('MouseEvents');
        evObj.initMouseEvent('click', true, true, window);
        node.dispatchEvent(evObj);
      }
      else {
        node.click();
      }
  }

    </script>

